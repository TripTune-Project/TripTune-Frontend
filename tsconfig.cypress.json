{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["node", "cypress/global"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
